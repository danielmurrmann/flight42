{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@flight42/aircraft-api": ["frontend/libs/aircraft/api/src/index.ts"],
      "@flight42/aircraft-domain": [
        "frontend/libs/aircraft/domain/src/index.ts"
      ],
      "@flight42/aircraft-feature-dashboard": [
        "frontend/libs/aircraft/feature-dashboard/src/index.ts"
      ],
      "@flight42/aircraft-ui-blocks": [
        "frontend/libs/aircraft/ui-blocks/src/index.ts"
      ],
      "@flight42/aircraft-ui-elements": [
        "frontend/libs/aircraft/ui-elements/src/index.ts"
      ],
      "@flight42/flight-domain": ["frontend/libs/flight/domain/src/index.ts"],
      "@flight42/flight-feature-manage": [
        "frontend/libs/flight/feature-manage/src/index.ts"
      ],
      "@flight42/flight-feature-next-flights": [
        "frontend/libs/flight/feature-next-flights/src/index.ts"
      ],
      "@flight42/flight-ui-design-system-blocks": [
        "frontend/libs/flight/ui-design-system-blocks/src/index.ts"
      ],
      "@flight42/flight-ui-design-system-elements": [
        "frontend/libs/flight/ui-design-system-elements/src/index.ts"
      ],
      "@flight42/shared-ui-design-system": [
        "frontend/libs/shared/ui-design-system/src/index.ts"
      ],
      "@flight42/shared-ui-design-system-blocks": [
        "frontend/libs/shared/ui-design-system-blocks/src/index.ts"
      ],
      "@flight42/shared-ui-design-system-elements": [
        "frontend/libs/shared/ui-design-system-elements/src/index.ts"
      ],
      "@flight42/shared-ui-design-system-layouts": [
        "frontend/libs/shared/ui-design-system-layouts/src/index.ts"
      ],
      "@flight42/shared-util": ["frontend/libs/shared/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
